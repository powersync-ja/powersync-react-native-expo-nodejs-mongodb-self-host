# ==================== PowerSync variables ====================
# The PowerSync API is accessible via this port
PS_PORT=8080
BACKEND_PORT=6060

# ==================== MongoDB credentials ================================
PS_DATA_SOURCE_URI=mongodb://mongo:27017/attendee-list

# ==================== Backend config =========================================
BACKEND_DATABASE_TYPE=mongodb
BACKEND_DATABASE_URI=${PS_DATA_SOURCE_URI}
POWERSYNC_URI=http://localhost:${PS_PORT}

PS_JWKS_URL=http://backend-api:${BACKEND_PORT}/api/auth/keys

# These can be generated by following the instructions in the `key-generator` folder
# A temporary key will be used if these are not specified
JWKS_PUBLIC_KEY=
JWKS_PRIVATE_KEY=


